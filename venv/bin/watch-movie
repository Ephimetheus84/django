#!/home/e/PycharmProjects/Python/django/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cinema==0.0.10','console_scripts','watch-movie'
__requires__ = 'cinema==0.0.10'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cinema==0.0.10', 'console_scripts', 'watch-movie')()
    )
